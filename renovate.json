{
  "extends": ["config:base"],
  "labels": ["renovate"],
  "enabledManagers": ["terraform"],
  "packageRules": [
    {
      "matchManagers": [
        "terraform"
      ],
      "groupName": "Terraform-Modules",
      "separateMajorMinor": false,
      "automerge": true,
      "ignoreTests": true,
      "ignoreUnstable": true
    }
  ],
  "repositories": [
    "steveizzle/terratest-experiment"
  ]
}
